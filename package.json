{
  "name": "template",
  "version": "1.0.0",
  "description": "Template to build a website",
  "main": "main.js",
  "author": "Characterr",
  "license": "MIT",
  "scripts": {
    "dev:assets": "webpack --watch",
    "dev": "live-server --open=./public/ --host=localhost --watch=./public/",
    "build": "NODE_ENV=production webpack",
    "prepare": "npx crlf --set=LF node_modules/.bin/live-server",
    "myLintPreCommit": "npx eslint public/**/*.js --color",
    "PreCommitFix": "npx eslint public/**/*.js --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.0",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "live-server": "^1.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss": "^8.4.20",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^7.8.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.2.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "keywords": [
    "JavaScript",
    "Webpack",
    "ES6+",
    "live reload",
    "Sass"
  ],
  "dependencies": {
    "moment": "^2.29.4"
  }
}
